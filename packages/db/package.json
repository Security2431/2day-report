{
  "name": "@acme/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "license": "MIT",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/prisma/seeds/index.ts"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "generate": "pnpm with-env prisma generate",
    "push": "pnpm with-env prisma db push --skip-generate",
    "studio": "pnpm with-env prisma studio --port 5556",
    "seed": "pnpm with-env prisma db seed",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.2.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base"
    ]
  },
  "prettier": "@acme/prettier-config"
}
